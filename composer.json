{
  "name": "tuncay/psalm-wp-taint",
  "version": "0.0.1-dev",
  "description": "A psalm plugin that adds stubs for psalm to define custom taint sinks for wordpress projects",
  "type": "psalm-plugin",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Tuncay\\PsalmWpTaint\\": [
        "."
      ]
    }
  },
  "authors": [
    {
      "name": "Tuncay Alarcin",
      "email": "tuncayalarcin@gmail.com"
    }
  ],
  "extra": {
    "psalm": {
      "pluginClass": "PsalmWpTaint\\Plugin"
    }
  },
  "require": {
    "vimeo/psalm": "dev-master",
    "ext-simplexml": "*",
    "nikic/php-parser": "dev-master",
    "adhocore/cli": "v1.7.1"
  },
  "require-dev": {
    "php-stubs/wordpress-stubs": "^6.5",
    "phpunit/phpunit": "^11"
  },
  "bin": [
    "analyze"
  ]
}
